<body ng-app="searchDoc" ng-cloak>
    <div ng-controller="searchDocController">
        <div class="container-fluid" >
            <!-- Creates the navbar -->
            <nav class="navbar navbar-default static-top">
                <!-- This creates the mechanism for collapse button -->
                <button type="button" class="navbar-toggle" data-toggle="collapse" ng-init="navCollapsed = true" ng-click="navCollapsed = !navCollapsed">
                    <!-- This controls the number of lines in the image when the nav collapse -->
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                <!-- This is the brand on the left-hand side. -->
                <a class="navbar-brand" href="/#"> Medool</a>
                <div class="collapse navbar-collapse" ng-class="!navCollapsed && 'in'">
                    <form ng-submit="submitLoginForm()" class="navbar-form navbar-right">
                        <div  class="form-group">
                            <select class="form-control" id="State">
                                <option value="Delhi">Delhi</option>
                                <option value="NCR">NCR</option>
                            </select>
                        </div>
                        <div class="form-group">

                            <input ng-model="loginForm.email" type="text" placeholder="Email" name="email" class="form-control">
                        </div>
                        <div class="form-group">
                            <input ng-model="loginForm.password" type="password" placeholder="Password" name="password" class="form-control">
                        </div>
                        <button type="submit" class="btn btn-success">
                            <span ng-show="!loginForm.loading">Sign in</span>
                            <span ng-show="loginForm.loading" class="fa fa-spinner"></span>
                            <span ng-show="loginForm.loading">Signing in...</span>
                        </button>
                        <input type="hidden" name="_csrf" value="<%= _csrf %>" />
                        <a href="signup" class="btn btn-success">Sign up now!</a>
                    </form>
            </nav>
        </div>

        <div class="main"  >
            <div class="wrapper">

                <!--            <input type="number" ng-model="gSlider.minValue" />
                          
                            <input type="number" ng-model="gSlider.maxValue" />-->
                <br/>
                <div class="form-group row">
                    <div class="col-sm-3 "></div>
                    <div class="col-sm-3 ">
                        <input type="text" placeholder="Location/Pincode" name="location" class="form-control" />
                    </div>

                    <div class="col-sm-3"	>
                        <div class="input-group">
                            <input type="text" list="doctors" name="search" class="form-control" placeholder="Specialisation, Doctor name or Clinic name " />

                            <span class="input-group-btn">
                                <button class="btn btn-success" type="button" ng-click="search()"><i class="fa fa-search"></i></button>
                            </span>
                        </div>
                    </div>
                    <div class="col-sm-3 "></div>
                </div>
                <div class="row" >
                    <div class="col-md-3">
                        Consultation Fee
                        <article>

                            <rzslider rz-slider-model="gSlider.minValue" rz-slider-high="gSlider.maxValue" rz-slider-options="gSlider.options"></rzslider>
                        </article>
                    </div>
                    <div class="col-md-3"> </div>
                    <div class="col-md-3">
                        Availability
                        <rzslider rz-slider-model="slider_alphabet.minValue" rz-slider-high="gSlider_alphabet.maxValue"
                                  rz-slider-options="slider_alphabet.options"></rzslider>
                    </div>
                    <div class="col-md-3">
                        <label>
                            <input type="radio" ng-model="gender.name" value="male">
                            Male
                        </label>
                        <label>
                            <input type="radio" ng-model="gender.name" ng-value="Female">
                            Female
                        </label><br/>

                    </div>
                </div>
            </div>
            <div class="container">



                <div ng-repeat="product in products| orderBy: 'consultation': false" class="col-md-4 col-sm-6 col-lg-4 col-xs-12">

                    <div class="thumbnail">
                        <div class="row">
                            <div class="col-md-4 col-xs-4 col-lg-4">
                                <img ng-src="{{ product.cover}}">
                            </div>

                            <div class="col-md-8">
                                <div>{{ product.name}}</div>
                                <div>{{ product.address}}</div>
                                <div>{{ product.type}}</div>
                                <div>{{ product.qualification}}</div>
                                <div>{{ product.experience}}</div>

                            </div>

                        </div>
                        <div class="clinic2">{{ product.clinic}}</div>

                        <div class="row ">
                            <div class="clinicphoto col-sm-3 col-xs-3 col-md-3 col-lg-3"><img  ng-src="{{ product.cphoto1}}" ></div>
                            <div class="clinicphoto col-sm-3 col-xs-3 col-md-3 col-lg-3"><img  ng-src="{{ product.cphoto2}}" ></div>
                            <div class="clinicphoto col-sm-3 col-xs-3 col-md-3 col-lg-3"><img  ng-src="{{ product.cphoto3}}" ></div>
                        </div>

                        <div class="row">
                            <div>Reviews {{ }}</div>
                            <div>Distance {{ product.distance}}</div>
                            <div>Consultation {{ product.consultation}}</div>
                        </div>
                        <div class="row">
                            <div class=" chat1 col-sm-3 col-xs-3 col-md-3">Video</div>
                            <div class=" chat1 col-sm-3 col-xs-3 col-md-3">Chat</div>
                            <div class=" chat1 col-sm-4 col-xs-4 col-md-4">Booking</div>
                        </div>


                        <!--  <p class="title">
                           {{ product.name }}
                         </p> -->

                        <p class="price">
                            {{ }}
                        </p>

                        <!-- <p class="date">
                          {{ product.pubdate | date }}
                        </p> -->
                        <div class="rating">

                            <p class="likes" ng-click="plusOne($index)">
                                +{{ product.likes}}
                            </p>

                        </div>
                    </div>

                </div>



            </div>
        </div>
    </div>
        <!--SCRIPTS-->
        <script src="/js/dependencies/sails.io.js"></script>
        <script src="/js/dependencies/moment.min.js"></script>
        <script src="/js/dependencies/interact.min.js"></script>
        <script src="/js/dependencies/angular.js"></script>
        <script src="/js/dependencies/angular-animate.js"></script>
        <script src="/js/dependencies/angular-bootstrap-calendar-tpls.min.js"></script>
        <script src="/js/dependencies/jquery.min.js"></script>
        <script src="/js/dependencies/angular-mocks.js"></script>
        <script src="/js/dependencies/angular-route.js"></script>
        <script src="/js/dependencies/angular-toastr.js"></script>
        <script src="/js/dependencies/angular-ui-router.min.js"></script>
        <script src="/js/dependencies/angular-ui.js"></script>
        <script src="/js/dependencies/angular.1.3.js"></script>
        <script src="/js/dependencies/bootstrap.js"></script>
        <script src="/js/dependencies/compareTo.module.js"></script>
        <script src="/js/dependencies/main.js"></script>
        <script src="/js/dependencies/metisMenu.min.js"></script>
        <script src="/js/dependencies/rzslider.js"></script>
        <script src="/js/dependencies/ui-bootstrap-tpls.min.js"></script>
        <script src="/js/dependencies/xeditable.js"></script>
        <script src="/js/public/signup/SignupModule.js"></script>
        <script src="/js/private/dashboard/DashboardModule.js"></script>
        <script src="/js/public/homepage/HomepageModule.js"></script>
        <script src="/js/public/search/searchDocModule.js"></script>
        <script src="/js/private/dashboard/BillingController.js"></script>
        <script src="/js/private/dashboard/DashboardController.js"></script>
        <script src="/js/private/dashboard/PersonalDetailController.js"></script>
        <script src="/js/private/dashboard/SchedulerController.js"></script>
        <script src="/js/private/dashboard/SchedulerService.js"></script>
        <script src="/js/private/dashboard/TimeTableController.js"></script>
        <script src="/js/private/dashboard/UserProfileController.js"></script>
        <script src="/js/private/dashboard/appointmentController.js"></script>
        <script src="/js/private/dashboard/messageController.js"></script>
        <script src="/js/private/dashboard/staffController.js"></script>
        <script src="/js/private/dashboard/toolsController.js"></script>
        <script src="/js/public/homepage/HomepageController.js"></script>
        <script src="/js/public/search/searchDocController.js"></script>
        <script src="/js/public/signup/SignupController.js"></script>
        <!--SCRIPTS END-->
        

</body>