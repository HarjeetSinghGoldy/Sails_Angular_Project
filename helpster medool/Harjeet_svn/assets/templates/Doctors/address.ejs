
<br>
     <form role="form" style="margin: 10px;" > 
	 <div class="form-group col-sm-6">
        <input type="text" class="form-control" name="clinic" placeholder="clinic name">
    </div>
	 <div class="form-group col-sm-6">
        <input type="text" class="form-control" name="clinicType" placeholder="clinic type">
    </div>
	 <div  class="form-group col-sm-6">
    <select class="form-control" name="state">
      <option value="">State</option> 
      <option value="male">Delhi</option> 
      <option value="female">NCR</option>     
       </select>
  </div>
  <div  class="form-group col-sm-6">
    <select class="form-control" name="district">
      <option value="">District</option> 
      <option value="male">South Delhi</option> 
      <option value="female">East delhi</option>
      <option value="female">North delhi</option>
     <option value="female">West delhi</option>     	  
       </select>
  </div>
    <div class="form-group col-sm-6">
        <input type="text" class="form-control" name="address1" placeholder="Address line-1">
     </div>
     <div class="form-group col-sm-6">
        <input type="text" class="form-control" name="city" placeholder="City/Village">
    </div>
     <div class="form-group col-sm-6">
        <input type="number" class="form-control" name="pincode" placeholder="PIN Code">
    </div>
	 <div class="form-group col-sm-6">
        <input type="number" class="form-control" name="phone" placeholder="Phone Number">
    </div>
         <div class="col-sm-12">
     <button type="button" class="btn btn-success">Update</button>
     </div>
  </form>

 