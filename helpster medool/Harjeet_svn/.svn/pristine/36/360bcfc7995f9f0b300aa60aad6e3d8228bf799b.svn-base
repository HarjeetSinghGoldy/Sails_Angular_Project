 <h2>Personal Details</h2>
  

 <form role="form" style="margin: 10px;" ng-cloak>
      
    <div class="form-group col-sm-6">
        <input type="text" class="form-control" name="fname" ng-model="user.firstname" placeholder="First Name">
    </div>
    <div class="form-group col-sm-6">
        <input type="text" class="form-control" name="lname" ng-model="user.lastname" placeholder="Last Name">
    </div>
       <div class="form-group col-sm-6">
        <input type="email" class="form-control" name="email" ng-model="user.email" placeholder="username@domain.com">
    </div>
    <div class="form-group col-sm-6">
        <input type="text" class="form-control" name="mobilenumber"ng-model="user.mobile" placeholder="Mobile number">
    </div>   
     <div  class="form-group col-sm-6">
    <select class="form-control" name="gender" ng-model="user.gender">
      <option value="">Gender</option> 
      <option value="male">Male</option> 
      <option value="female">Female</option>     
       </select>
  </div>
     <div class="col-sm-6">
      <div class="form-group form-inline row" style="margin-left:0px; margin-right:0px;">
    
   <div class="form-group form-inline " style="margin-left:0px; margin-right:0px;">
    <select class="form-control col-sm-4"  ng-model="date.day" ng-options="day for day in days" >
        <option value="0">Day</option>
                            </select>
</div>

<div class="form-group form-inline " style="margin-left:0px; margin-right:0px;">
                            
                            <select class="form-control col-sm-4" ng-model="date.month"
 ng-options="month for month in months">
                                <option value="0">Month</option>
 </select>
 </div>
 <div class="form-group form-inline " style="margin-left:0px; margin-right:0px;">
    <select aria-label="Year" class="form-control col-sm-4" ng-model="date.year" ng-options="year for year in years" selected="1">
      <option value="0">Year</option>
          </select>
        </div>
      </div>
     </div>
     <div class="col-sm-12">
     <button type="button" class="btn btn-success">Update</button>
     </div>
  </form>


